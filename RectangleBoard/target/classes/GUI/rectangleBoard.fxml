<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.effect.Light.Distant?>
<?import javafx.scene.effect.Lighting?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.layout.AnchorPane?>


<TabPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RectangleBoardController">
   <tabs>
      <Tab text="Cell editor">
         <content>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0">
            	<children>
                  <AnchorPane fx:id="board" prefHeight="600.0" prefWidth="600.0" />
                  <JFXButton fx:id="button" defaultButton="true" layoutX="661.0" layoutY="114.0" onAction="#display" text="Load">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </JFXButton>
                  <JFXTextField layoutX="620.0" layoutY="72.0" promptText="Enter file name" />
                  <JFXToggleButton layoutX="616.0" layoutY="14.0" text="Edit">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </JFXToggleButton>
                  <JFXButton layoutX="662.0" layoutY="182.0" text="Start" />
                  <JFXButton layoutX="662.0" layoutY="235.0" text="Stop" />
                  <JFXButton layoutX="659.0" layoutY="300.0" text="Pause" />
            	</children>
            </AnchorPane>
         </content>
      </Tab>
      <Tab text="Paint editor">
         <content>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0">
               <children>
                  <AnchorPane fx:id="board1" prefHeight="600.0" prefWidth="600.0" />
                  <JFXButton fx:id="button1" defaultButton="true" layoutX="661.0" layoutY="114.0" onAction="#display" text="Load">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </JFXButton>
                  <JFXTextField layoutX="620.0" layoutY="72.0" promptText="Enter file name" />
                  <JFXToggleButton layoutX="616.0" layoutY="14.0" text="Edit">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </JFXToggleButton>
                  <JFXButton layoutX="662.0" layoutY="182.0" text="Start" />
                  <JFXButton layoutX="662.0" layoutY="235.0" text="Stop" />
                  <JFXButton layoutX="659.0" layoutY="300.0" text="Pause" />
               </children>
            </AnchorPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
